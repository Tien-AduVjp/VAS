<odoo>
	<record id="stock_picking_backdate_view_form" model="ir.ui.view">
		<field name="name">stock.picking.backdate.form</field>
		<field name="model">stock.picking.backdate</field>
		<field name="inherit_id"
			ref="to_backdate.abstract_backdate_wizard_view_form" />
		<field name="mode">primary</field>
		<field name="arch" type="xml">
			<xpath expr="//field[@name='date']" position="after">
				<field name="picking_id" invisible="1" />
			</xpath>
		</field>
	</record>
</odoo>
