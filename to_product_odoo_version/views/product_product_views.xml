<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record model="ir.ui.view" id="product_variant_easy_edit_view">
		<field name="name">product</field>
		<field name="model">product.product</field>
		<field name="inherit_id"
			ref="product.product_variant_easy_edit_view"/>
		<field name="arch" type="xml">
			<xpath expr="//div[hasclass('oe_title')]"
				position="after">
				<group string="Odoo Versions">
					<field name="odoo_version_ids" widget="many2many_tags"/>
				</group>
			</xpath>
		</field>
	</record>
</odoo>

