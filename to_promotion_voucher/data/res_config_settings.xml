<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="res.config.settings" id="stock_config_settings_promotion_voucher">
            <!-- Enable Lots & Serial Numbers -->
            <field name="group_stock_production_lot" eval="1"/>
            <!-- Enable Storage Locations -->
            <field name="group_stock_multi_locations" eval="1"/>
            <!-- Enable Multi-Warehouses -->
            <field name="group_stock_multi_warehouses" eval="1"/>
            <!-- Enable Multi-Step Routes -->
            <field name="group_stock_adv_location" eval="1"/>
        </record>

        <function model="res.config.settings" name="execute">
            <value eval="[ref('to_promotion_voucher.stock_config_settings_promotion_voucher')]"/>
        </function>
    </data>
</odoo>
