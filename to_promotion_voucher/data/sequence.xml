<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_issue_release_order" model="ir.sequence">
            <field name="name">Voucher Issue Order</field>
            <field name="code">voucher.issue.order</field>
            <field name="prefix">VIO</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_voucher_move_order" model="ir.sequence">
            <field name="name">Voucher Move Order</field>
            <field name="code">voucher.move.order</field>
            <field name="prefix">VMO</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_voucher_give_order" model="ir.sequence">
            <field name="name">Voucher Give Order</field>
            <field name="code">voucher.give.order</field>
            <field name="prefix">VGO</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>
