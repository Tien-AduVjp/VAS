<odoo>
	<data noupdate="0">
		<record id="payment_acquirer_nganluong"
			model="payment.acquirer">
			<field name="name">NganLuong</field>
			<field name="provider">nganluong</field>
			<field name="company_id" ref="base.main_company" />
			<field name="sequence">1</field>
			<field name="fees_dom_var" eval="1.0" />
			<field name="fees_dom_fixed" eval="1000.0" />
			<field name="fees_int_var" eval="1.0" />
			<field name="fees_int_fixed" eval="1000.0" />
			<field name="view_template_id"
				ref="payment_nganluong.nganluong_form" />
			<field name="image_128" type="base64"
				file="payment_nganluong/static/src/img/nganluong_icon.png" />
			<field name="module_id" ref="base.module_payment_nganluong" />
			<field name="description" type="html">
				<p>
					Payments via NganLuong or credit cards.
				</p>
				<ul class="list-inline">
					<li class="list-inline-item">
						<i class="fa fa-check" />
						Online NganLuong
					</li>
					<li class="list-inline-item">
						<i class="fa fa-check" />
						Payment Status Tracking
					</li>
				</ul>
			</field>
			<field name="payment_icon_ids"
				eval='[(6, 0, [
							ref("payment.payment_icon_cc_mastercard"),
							ref("payment.payment_icon_cc_visa"),
							ref("to_vietnam_bank_icons.payment_icon_bank_vcb"),
							ref("to_vietnam_bank_icons.payment_icon_bank_acb"),
							ref("to_vietnam_bank_icons.payment_icon_bank_bidv"),
							ref("to_vietnam_bank_icons.payment_icon_bank_dab"),
							ref("to_vietnam_bank_icons.payment_icon_bank_exb"),
							ref("to_vietnam_bank_icons.payment_icon_bank_icb"),
							ref("to_vietnam_bank_icons.payment_icon_bank_MB"),
							ref("to_vietnam_bank_icons.payment_icon_bank_msb"),
							ref("to_vietnam_bank_icons.payment_icon_bank_stb"),
							ref("to_vietnam_bank_icons.payment_icon_bank_tcb"),
							ref("to_vietnam_bank_icons.payment_icon_bank_vib"),
							])
						]' />
			<field name="country_ids" eval="[(6,0,[ref('base.vn')])]" />
		</record>
	</data>
</odoo>
