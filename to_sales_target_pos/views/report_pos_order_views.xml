<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record model="ir.ui.view" id="view_report_pos_order_search">
			<field name="name">report.pos.order.search</field>
			<field name="model">report.pos.order</field>
			<field name="inherit_id" ref="point_of_sale.view_report_pos_order_search" />
			<field name="arch" type="xml">				
				<xpath expr="//field[@name='partner_id']" position="after">
					<field name="crm_team_id"/>
				</xpath>
				<xpath expr="//group/filter[@name='User']" position="after">
					<filter string="Team/Channel" name="grp_crm_team_id" context="{'group_by':'crm_team_id'}"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>
