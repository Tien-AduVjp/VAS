<?xml version="1.0" encoding="utf-8" ?>

<odoo>
	<record id="update_timesheet_cost_form_view" model="ir.ui.view">
		<field name="name">timesheet.cost.update.form</field>
		<field name="model">timesheet.cost.update</field>
		<field name="arch" type="xml">
			<form string="Update timesheet cost">
				<group>
					<h3>Do you want update time sheet cost?</h3>
				</group>
				<footer>
					<button name="update_timesheet_cost" string="Confirm" type="object" class="btn-primary" />
					<button string="Cancel" class="btn-default" special="cancel" />
				</footer>
			</form>
		</field>
	</record>
	
	<act_window name="Update Existing Timesheet Cost"
		res_model="timesheet.cost.update" binding_model="hr.payslip"
		id="act_update_timesheet" view_mode="form" target="new" />
</odoo>