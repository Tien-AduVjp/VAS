<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="document_workspace_hr_payroll" model="document.workspace">
        <field name="name">Payroll</field>
        <field name="tag_ids" eval="[
            (4, ref('document_tag_hr_payroll_payslip_batch')),
            (4, ref('document_tag_hr_payroll_payslip')),
            (4, ref('viin_document.document_tag_to_review')),
            (4, ref('viin_document.document_tag_approve')),
            (4, ref('viin_document.document_tag_deprecate'))]"/>
        <field name="read_team_ids" eval="[(4, ref('document_team_hr_payroll'))]"/>
        <field name="write_group_ids" eval="[(4, ref('to_hr_payroll.group_hr_payroll_manager'))]"/>
        <field name="read_group_ids" eval="[(4, ref('to_hr_payroll.group_hr_payroll_user'))]"/>
        <field name="action_ids" eval="[
            (4, ref('viin_document.document_action_ask_to_review')),
            (4, ref('viin_document.document_action_approve'))]"/>
    </record>

</odoo>
