<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="hr_employee_public_view_viin_map" model="ir.ui.view">
		<field name="name">hr.employee.public.view.viin_map</field>
		<field name="model">hr.employee.public</field>
		<field name="arch" type="xml">
			<viin_map res_partner="address_id">
				<marker-popup>
					<field name="name" string="Name " />
					<field name="address_id" string="Address " />
				</marker-popup>
			</viin_map>
		</field>
	</record>

	<record id="view_hr_employee_public_action_view_kanban"
		model="ir.actions.act_window.view">
		<field name="sequence" eval="0" />
		<field name="view_mode">kanban</field>
		<field name="view_id" ref="hr.hr_employee_public_view_kanban" />
		<field name="act_window_id" ref="hr.hr_employee_public_action" />
	</record>

	<record id="view_hr_employee_public_action_view_list"
		model="ir.actions.act_window.view">
		<field name="sequence" eval="10" />
		<field name="view_mode">tree</field>
		<field name="view_id" ref="hr.hr_employee_public_view_tree" />
		<field name="act_window_id" ref="hr.hr_employee_public_action" />
	</record>

	<record id="view_hr_employee_public_action_view_viin_map"
		model="ir.actions.act_window.view">
		<field name="sequence" eval="20" />
		<field name="view_mode">viin_map</field>
		<field name="view_id" ref="hr_employee_public_view_viin_map" />
		<field name="act_window_id" ref="hr.hr_employee_public_action" />
	</record>

</odoo>
