<?xml version="1.0" encoding="UTF-8"?>
<odoo>

	<record id="account_tag_action" model="ir.actions.act_window">
		<field name="name">Account Tags</field>
		<field name="res_model">account.account.tag</field>
		<field name="view_mode">tree,form</field>
		<field name="search_view_id"
			ref="account.account_tag_view_search" />
		<field name="help" type="html">
			<p class="o_view_nocontent_smiling_face">
				Add a new tag
			</p>
		</field>
	</record>

	<menuitem id="account_tag_menu"
		parent="account.account_account_menu"
		action="to_account_accountant.account_tag_action"
		groups="base.group_no_one" />

</odoo>
