<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record model="ir.ui.view" id="fleet_vehicle_costs_view_form">
			<field name="name">fleet.vehicle.cost.form</field>
			<field name="model">fleet.vehicle.cost</field>
			<field name="inherit_id" ref="fleet.fleet_vehicle_costs_view_form" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='parent_id']" position="after">
					<field name="created_from_stock_move_id" readonly="True"
						attrs="{'invisible': [('created_from_stock_move_id','=',False)]}" />
					<field name="created_from_stock_stock_picking_id" readonly="True"
						attrs="{'invisible': [('created_from_stock_stock_picking_id','=',False)]}" />
				</xpath>
			</field>
		</record>
	</data>
</odoo>