<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record model="ir.ui.view" id="view_move_form">
			<field name="name">stock.move.form</field>
			<field name="model">stock.move</field>
			<field name="inherit_id" ref="stock.view_move_form" />
			<field name="arch" type="xml">
				<xpath expr="//group[@name='origin_grp']" position="after">
					<group name="origin_grp" string="Fleet">
						<field name="fleet_service_type_id" readonly="True" />
						<field name="vehicle_id" readonly="True" />
					</group>
				</xpath>
			</field>
		</record>
		
		<record model="ir.ui.view" id="view_move_picking_tree">
			<field name="name">stock.move.tree</field>
			<field name="model">stock.move</field>
			<field name="inherit_id" ref="stock.view_move_picking_tree" />
			<field name="arch" type="xml">
				<xpath expr="///field[@name='product_uom_qty']" position="after">
					<field name="vehicle_id" />
				</xpath>
			</field>
		</record>
	</data>
</odoo>