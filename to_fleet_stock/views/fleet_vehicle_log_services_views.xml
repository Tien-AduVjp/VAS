<odoo>
		<record id="fleet_vehicle_log_services_view_form" model="ir.ui.view" >
			<field name="name">fleet.vehicle.log.services.form.inherit</field>
			<field name="model">fleet.vehicle.log.services</field>
			<field name="inherit_id" ref="fleet.fleet_vehicle_log_services_view_form" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='purchaser_id']/parent::group" position="inside">
					<field name="created_from_stock_move_id" readonly="True"
						attrs="{'invisible': [('created_from_stock_move_id','=',False)]}" />
					<field name="created_from_stock_stock_picking_id" readonly="True"
						attrs="{'invisible': [('created_from_stock_stock_picking_id','=',False)]}" />
				</xpath>
			</field>
		</record>
</odoo>
