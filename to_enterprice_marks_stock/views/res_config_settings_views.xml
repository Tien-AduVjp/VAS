<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="res_config_settings_view_form" model="ir.ui.view">
			<field name="name">res.config.settings.form</field>
			<field name="model">res.config.settings</field>
			<field name="inherit_id"
				ref="stock.res_config_settings_view_form" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='module_stock_barcode']"
					position="replace">
					<field name="module_to_stock_barcode" />
				</xpath>

				<xpath expr="//label[@for='module_stock_barcode']"
					position="attributes">
					<attribute name="for">module_to_stock_barcode</attribute>
				</xpath>

			</field>
		</record>

	</data>
</odoo>
