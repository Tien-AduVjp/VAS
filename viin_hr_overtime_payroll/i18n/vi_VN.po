msgid ""
"For overtime extra pay (as non taxed):\n"
"\t\t\t\t\t\t\t<br/>\n"
"\t\t\t\t\t\t\t<code>\n"
"\t\t\t\t\t\t\t\tactual_overtime_pay =\n"
"\t\t\t\t\t\t\t\tsum(payslip.overtime_plan_line_ids.mapped('actual_overtime_pay')))\n"
"\t\t\t\t\t\t\t\t<br/>\n"
"\t\t\t\t\t\t\t\tstandard_pay =\n"
"\t\t\t\t\t\t\t\tsum(payslip.overtime_plan_line_ids.mapped('standard_pay')))\n"
"\t\t\t\t\t\t\t\t<br/>\n"
"\t\t\t\t\t\t\t\tresult = actual_overtime_pay - standard_pay\n"
"\t\t\t\t\t\t\t</code>"
msgstr ""
"Để tính phần lương tăng ca phụ trội (không chịu thuế):\n"
"\t\t\t\t\t\t\t<br/>\n"
"\t\t\t\t\t\t\t<code>\n"
"\t\t\t\t\t\t\t\tactual_overtime_pay =\n"
"\t\t\t\t\t\t\t\tsum(payslip.overtime_plan_line_ids.mapped('actual_overtime_pay')))\n"
"\t\t\t\t\t\t\t\t<br/>\n"
"\t\t\t\t\t\t\t\tstandard_pay =\n"
"\t\t\t\t\t\t\t\tsum(payslip.overtime_plan_line_ids.mapped('standard_pay')))\n"
"\t\t\t\t\t\t\t\t<br/>\n"
"\t\t\t\t\t\t\t\tresult = actual_overtime_pay - standard_pay\n"
"\t\t\t\t\t\t\t</code>"

#. module: viin_hr_overtime_payroll
#: model_terms:ir.ui.view,arch_db:viin_hr_overtime_payroll.view_hr_payslip_form
msgid ""
"For total overtime pay amount based on standard contract rate *\n"
"\t\t\t\t\t\t\tovertime pay rate:\n"
"\t\t\t\t\t\t\t<br/>\n"
"\t\t\t\t\t\t\t<code>\n"
"\t\t\t\t\t\t\t\tresult=sum(payslip.overtime_plan_line_ids.mapped('actual_overtime_pay')))\n"
"\t\t\t\t\t\t\t</code>"
msgstr ""
"Để tính phần lương tăng ca theo công thức lương hợp đồng *\n"
"\t\t\t\t\t\t\ttỷ lệ chi trả:\n"
"\t\t\t\t\t\t\t<br/>\n"
"\t\t\t\t\t\t\t<code>\n"
"\t\t\t\t\t\t\t\tresult=sum(payslip.overtime_plan_line_ids.mapped('actual_overtime_pay')))\n"
"\t\t\t\t\t\t\t</code>"

#. module: viin_hr_overtime_payroll
