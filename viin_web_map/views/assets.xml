<odoo>
    <template id="assets_backend" name="viin_map assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/viin_web_map/static/src/js/viin_map_model.js"/>
            <script type="text/javascript" src="/viin_web_map/static/src/js/viin_map_renderer.js"/>
            <script type="text/javascript" src="/viin_web_map/static/src/js/viin_map_controller.js"/>
            <script type="text/javascript" src="/viin_web_map/static/src/js/viin_map_view.js"/>
            <link rel="stylesheet" type="text/css" href="/viin_web_map/static/lib/leaflet/leaflet.css"/>
            <link rel="stylesheet" type="text/scss" href="/viin_web_map/static/src/scss/viinmapview.scss"/>
        </xpath>
    </template>

    <template id="qunit_suite" name="viiin web map tests" inherit_id="web.qunit_suite_tests">
        <xpath expr="//script[last()]" position="after">
        	<script type="text/javascript" src="/viin_web_map/static/tests/helpers/test_utils_mock.js"/>
        	<script type="text/javascript" src="/viin_web_map/static/tests/helpers/test_utils_create.js"/>
            <script type="text/javascript" src="/viin_web_map/static/tests/helpers/test_utils.js"/>
            <script type="text/javascript" src="/viin_web_map/static/tests/viin_map_test.js"/>
        </xpath>
    </template>
</odoo>
