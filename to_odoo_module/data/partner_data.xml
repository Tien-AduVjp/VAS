<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<!-- res.partner data -->
	<data noupdate="1">
		<!-- Odoo S.A. -->
		<record id="res_partner_odoo_sa" model="res.partner">
			<field name="name">Odoo S.A.</field>
			<field name="is_company">1</field>
			<field name="city">Grand-Rosi√®re</field>
			<field name="zip">1367</field>
			<field name="country_id" ref="base.be" />
			<field name="street">Rue des Bourlottes, 9</field>
			<field name="website">https://www.odoo.com</field>
