<odoo noupdate="1">
	<!-- HR Advance Sequence -->
	<record id="seq_employee_advance" model="ir.sequence">
		<field name="name">HR Advance</field>
		<field name="code">employee.advance</field>
		<field name="prefix">AD</field>
		<field name="padding">6</field>
		<field name="company_id" eval="False" />
	</record>
	<!-- HR Advance Reconcile Sequence -->
	<record id="seq_employee_advance_reconcile" model="ir.sequence">
		<field name="name">HR Advance Reconcile</field>
		<field name="code">employee.advance.reconcile</field>
		<field name="prefix">EAR</field>
		<field name="padding">6</field>
		<field name="company_id" eval="False" />
	</record>
</odoo>
