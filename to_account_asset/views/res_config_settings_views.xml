<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.account.asset</field>
        <field name="model">res.config.settings</field>
        <field name="priority">110</field>
        <field name="inherit_id" ref="to_enterprise_marks_account.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div id="msg_to_account_asset" position="replace">
                <div class="content-group" attrs="{'invisible': [('module_to_account_asset', '=', False)]}">
                    <div class="mt16">
                        <button name="%(to_account_asset.account_asset_category_purchase_action)d" icon="fa-arrow-right" type="action" string="Asset Categories" class="btn-link"/>
                    </div>
                </div>
            </div>
        </field>
    </record>

</odoo>
