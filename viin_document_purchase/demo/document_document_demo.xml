<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="document_document_PO0001_demo" model="document.document">
        <field name="name">RFQ PO0001</field>
        <field name="datas" type="base64" file="viin_document_purchase/demo/documents/RFQ - P00001.pdf"/>
        <field name="workspace_id" ref="document_workspace_purchase"/>
        <field name="owner_id" ref="base.user_admin"/>
        <field name="tag_ids" eval="[
            (4, ref('document_tag_rfq')),
            (4, ref('viin_document.document_tag_to_review'))]"/>
    </record>

    <record id="document_document_PO0002_demo" model="document.document">
        <field name="name">Purchase Order PO0001</field>
        <field name="datas" type="base64" file="viin_document_purchase/demo/documents/PurchaseOrder - P00002.pdf"/>
        <field name="workspace_id" ref="document_workspace_purchase"/>
        <field name="owner_id" ref="base.user_demo"/>
        <field name="tag_ids" eval="[
            (4, ref('document_tag_purchase_order')),
            (4, ref('viin_document.document_tag_approve'))]"/>
    </record>

</odoo>
