<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>


		<record id="to_c200_s03adn_form" model="ir.ui.view">
			<field name="name">C200 S03a-DN Form</field>
			<field name="model">wizard.l10n_vn.c200_s03adn</field>
			<field name="inherit_id"
				ref="account.account_common_report_view" />
			<field name="arch" type="xml">
				<data>
					<xpath expr="//button[@name='check_report']"
						position="attributes">
						<attribute name="string">Print Pdf</attribute>
					</xpath>
					<xpath expr="//button[@name='check_report']" position="after">
						<button name="check_report_excel" string="Print Excel"
							type="object" class="oe_highlight" />
					</xpath>
				</data>
			</field>
		</record>


		<record id="action_wizard_c200_s03adn"
			model="ir.actions.act_window">
			<field name="name">GENERAL LEDGER (S03a-DN)</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">wizard.l10n_vn.c200_s03adn</field>
			<field name="view_mode">form</field>
			<field name="view_id" ref="to_c200_s03adn_form" />
			<field name="target">new</field>
		</record>

		<menuitem parent="l10n_vn.account_reports_vn_statements_menu"
			id="to_menu_general_ledger_report"
			groups="account.group_account_user,account.group_account_manager"
			action="action_wizard_c200_s03adn" sequence="2"
			name="General Ledger (S03a-DN)" />

	</data>
</odoo>