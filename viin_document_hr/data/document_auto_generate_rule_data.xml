<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="document_auto_generate_rule_hr_employee" model="document.auto.generate.rule" forcecreate="False">
        <field name="res_model_id" ref="hr.model_hr_employee"/>
        <field name="parent_workspace_id" ref="document_workspace_hr_document"/>
        <field name="workspace_tag_ids" eval="[
            (4, ref('document_tag_hr_employee_document')),
            (4, ref('document_tag_hr_certification')),
            (4, ref('document_tag_hr_hr')),
            (4, ref('viin_document.document_tag_to_review')),
            (4, ref('viin_document.document_tag_approve')),
            (4, ref('viin_document.document_tag_deprecate'))]"/>
        <field name="workspace_read_team_ids" eval="[(4, ref('document_team_hr'))]"/>
        <field name="workspace_write_group_ids" eval="[(4, ref('hr.group_hr_manager'))]"/>
        <field name="workspace_read_group_ids" eval="[(4, ref('hr.group_hr_user'))]"/>
        <field name="workspace_action_ids" eval="[
            (4, ref('viin_document.document_action_ask_to_review')),
            (4, ref('viin_document.document_action_approve')),
            (4, ref('viin_document.document_action_send_to_legal')),
            (4, ref('viin_document.document_action_deprecated'))]"/>
    </record>

</odoo>
