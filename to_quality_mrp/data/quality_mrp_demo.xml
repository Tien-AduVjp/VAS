<odoo>
    <!-- Quality Control From Manufacturing -->
    <!-- For [FURN_9666] Table-->
    <function model="quality.point" name="create">
            <value model="quality.point" eval="dict(
                obj().default_get(list(obj().fields_get())),
                **{
                	'type_id': ref('viin_quality_product.quality_type_product'),
                    'product_id': ref('mrp.product_product_computer_desk'),
                    'product_tmpl_id': ref('mrp.product_product_computer_desk_product_template'),
                    'picking_type_id': obj().env['stock.picking.type'].search([('code', '=', 'mrp_operation')], limit=1).id,
                }
            )"/>
    </function>

