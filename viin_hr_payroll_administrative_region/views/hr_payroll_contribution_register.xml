<odoo>
	<record id="hr_payroll_contribution_register_form_view" model="ir.ui.view">
		<field name="name">Payroll Contribution Register</field>
		<field name="model">hr.payroll.contribution.register</field>
		<field name="inherit_id" ref="to_hr_payroll.hr_payroll_contribution_register_form_view" />
		<field name="arch" type="xml">
			<field name="partner_id" position="before">
				<label for="max_contribution_employee"/>
                <div class="o_row" name="max_contribution_employee">
                    <field name="max_contribution_employee" nolabel="1"/>
                    <span>/ month</span>
                </div>
                <label for="max_contribution_company"/>
                <div class="o_row" name="max_contribution_company">
                    <field name="max_contribution_company" nolabel="1"/>
                    <span>/ month</span>
                </div>
			</field>
		</field>
	</record>
</odoo>
