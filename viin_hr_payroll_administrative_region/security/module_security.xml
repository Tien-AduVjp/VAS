<odoo>
     <record id="to_hr_payroll.group_hr_payroll_manager" model="res.groups">
		<field name="implied_ids"
			eval="[(4, ref('viin_administrative_region.group_administrative_region_manager'))]" />
	</record>

     <record id="admin_region_payroll_contrib_comp_rule" model="ir.rule">
		<field name="name">Administrative Region Payroll Contribution</field>
		<field name="model_id" ref="model_admin_region_payroll_contrib" />
