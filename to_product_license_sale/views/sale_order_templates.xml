<odoo>
	<data>
		<template id="report_saleorder_document"
			inherit_id="sale.report_saleorder_document">
			<xpath expr="//p[@id='fiscal_position_remark']"
				position="after">
				<t t-foreach="doc.product_license_version_ids" t-as="license">
					<h3>
						<span t-field="license.license_id.name" />
						<span>version</span>
						<span t-field="license.name" />
					</h3>
					<div>
						<span t-field="license.content" />
					</div>
				</t>
			</xpath>
		</template>
	</data>
</odoo>