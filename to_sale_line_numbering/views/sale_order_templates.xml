<odoo>
    <data>
    	<template id="report_saleorder_document" inherit_id="sale.report_saleorder_document">
    		<xpath expr="//table[hasclass('table') and hasclass('o_main_table')][1]/thead/tr/th[1]" position="before">
    			<th class="text-right" name="seq_nbr">#</th>
    		</xpath>
    		<xpath expr="//td[./span[@t-field='line.name']][1]" position="before">
    			<td class="text-right" name="seq_nbr"><span t-field="line.sequence"/></td>
    		</xpath>
    	</template>
    </data>
</odoo>