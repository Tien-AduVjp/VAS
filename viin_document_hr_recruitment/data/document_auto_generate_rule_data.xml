<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="document_auto_generate_rule_hr_applicant" model="document.auto.generate.rule" forcecreate="False">
        <field name="res_model_id" ref="hr_recruitment.model_hr_applicant"/>
        <field name="workspace_field_id" ref="hr_recruitment.field_hr_applicant__job_id"/>
        <field name="parent_workspace_id" ref="document_workspace_hr_recruitment"/>
        <field name="workspace_tag_ids" eval="[
            (4, ref('viin_document_hr.document_tag_hr_employee_document')),
            (4, ref('viin_document_hr.document_tag_hr_certification')),
            (4, ref('viin_document_hr.document_tag_hr_hr')),
            (4, ref('document_tag_hr_recruitment_cv')),
            (4, ref('document_tag_hr_recruitment_application_letter')),
            (4, ref('document_tag_hr_recruitment_recruiting')),
            (4, ref('document_tag_hr_recruitment_done')),
            (4, ref('document_tag_hr_recruitment_cancel')),
            (4, ref('viin_document.document_tag_to_review')),
            (4, ref('viin_document.document_tag_approve')),
            (4, ref('viin_document.document_tag_deprecate'))]"/>
        <field name="workspace_read_team_ids" eval="[(4, ref('document_team_hr_recruitment'))]"/>
        <field name="workspace_write_group_ids" eval="[(4, ref('hr.group_hr_manager'))]"/>
        <field name="workspace_read_group_ids" eval="[(4, ref('hr.group_hr_user'))]"/>
        <field name="workspace_action_ids" eval="[
            (4, ref('document_action_hr_recruitment_done')),
            (4, ref('document_action_hr_recruitment_cancel')),
            (4, ref('viin_document.document_action_ask_to_review')),
            (4, ref('viin_document.document_action_approve')),
            (4, ref('viin_document.document_action_send_to_legal')),
            (4, ref('viin_document.document_action_deprecated'))]"/>
    </record>

</odoo>
