<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="document_tag_hr_recruitment_cv" model="document.tag">
        <field name="name">CV</field>
        <field name="category_ids" eval="[(4, ref('document_tag_category_hr_recruitment'))]"/>
    </record>

    <record id="document_tag_hr_recruitment_application_letter" model="document.tag">
        <field name="name">Application Letter</field>
        <field name="category_ids" eval="[(4, ref('document_tag_category_hr_recruitment'))]"/>
    </record>

    <record id="document_tag_hr_recruitment_recruiting" model="document.tag">
        <field name="name">Recruiting</field>
        <field name="category_ids" eval="[(4, ref('document_tag_category_hr_recruitment_status'))]"/>
    </record>

    <record id="document_tag_hr_recruitment_done" model="document.tag">
        <field name="name">Recruitment Done</field>
        <field name="category_ids" eval="[(4, ref('document_tag_category_hr_recruitment_status'))]"/>
    </record>

    <record id="document_tag_hr_recruitment_cancel" model="document.tag">
        <field name="name">Recruitment Cancelled</field>
        <field name="category_ids" eval="[(4, ref('document_tag_category_hr_recruitment_status'))]"/>
    </record>

</odoo>
