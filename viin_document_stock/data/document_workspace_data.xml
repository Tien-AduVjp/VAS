<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="document_workspace_inventory" model="document.workspace">
        <field name="name">Inventory</field>
        <field name="tag_ids" eval="[
            (4, ref('document_tag_stock_delivery')),
            (4, ref('document_tag_stock_receipt')),
            (4, ref('document_tag_stock_waiting_another_operation')),
            (4, ref('document_tag_stock_ready')),
            (4, ref('document_tag_stock_done')),
            (4, ref('viin_document.document_tag_to_review')),
            (4, ref('viin_document.document_tag_approve')),
            (4, ref('viin_document.document_tag_deprecate'))]"/>
        <field name="read_team_ids" eval="[(4, ref('document_team_inventory'))]"/>
        <field name="write_group_ids" eval="[(4, ref('stock.group_stock_manager'))]"/>
        <field name="read_group_ids" eval="[(4, ref('stock.group_stock_user'))]"/>
        <field name="action_ids" eval="[
            (4, ref('viin_document.document_action_ask_to_review')),
            (4, ref('viin_document.document_action_approve')),
            (4, ref('document_action_stock_waiting')),
            (4, ref('document_action_stock_mark_as_ready')),
            (4, ref('document_action_stock_mark_as_done'))]"/>
    </record>

</odoo>
