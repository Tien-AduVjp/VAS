<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="0">
		<!-- Resource: uom.category -->
		<record id="uom_categ_subscription" model="uom.category">
			<field name="name">Subscription</field>
			<field name="measure_type">subscription</field>
			<field name="concat_uom_name" eval="True" />
		</record>
		
		<record id="uom_categ_subscription_user" model="uom.category">
			<field name="name">User Subscription</field>
		</record>
		
		<!-- Resource: uom.uom -->
		<record id="uom_subscription_day" model="uom.uom">
			<field name="name">Day(s)</field>
			<field eval="uom_categ_subscription" name="category_id" />
			<field name="factor" eval="1.0" />
			<field name="rounding" eval="0.001" />
		</record>
		<record id="subscription_uom_week" model="uom.uom">
			<field name="name">Week(s)</field>
			<field ref="uom_categ_subscription" name="category_id" />
			<field name="factor_inv" eval="7" />
			<field name="uom_type">bigger</field>
			<field name="rounding" eval="0.001" />
		</record>
		<record id="uom_subscription_month" model="uom.uom">
			<field name="name">Month(s)</field>
			<field eval="uom_categ_subscription" name="category_id" />
			<field name="factor_inv" eval="28" />
			<field name="uom_type">bigger</field>
			<field name="rounding" eval="0.001" />
		</record>
		<record id="uom_subscription_year" model="uom.uom">
			<field name="name">Year(s)</field>
			<field eval="uom_categ_subscription" name="category_id" />
			<field name="factor_inv" eval="336" />
			<field name="uom_type">bigger</field>
			<field name="rounding" eval="0.001" />
		</record>
		<record id="uom_subscription_hour" model="uom.uom">
			<field name="name">Hour(s)</field>
			<field eval="uom_categ_subscription" name="category_id" />
			<field name="factor" eval="24" />
			<field name="uom_type">smaller</field>
		</record>
		
		<!-- User Subscription -->
		<record id="uom_subscription_user_day" model="uom.uom">
			<field name="name">User/Day</field>
			<field name="category_id" ref="uom_categ_subscription_user" />
			<field name="factor" eval="1.0" />
			<field name="rounding" eval="0.001" />
		</record>
		<record id="uom_subscription_user_month" model="uom.uom">
			<field name="name">User/Month</field>
			<field name="category_id" ref="uom_categ_subscription_user" />
			<field name="factor_inv" eval="28" />
			<field name="uom_type">bigger</field>
			<field name="rounding" eval="0.001" />
		</record>
		<record id="uom_subscription_user_year" model="uom.uom">
			<field name="name">User/Year</field>
			<field name="category_id" ref="uom_categ_subscription_user" />
			<field name="factor_inv" eval="336" />
			<field name="uom_type">bigger</field>
			<field name="rounding" eval="0.001" />
		</record>
		<record id="uom_subscription_user_hour" model="uom.uom">
			<field name="name">User/Hour</field>
			<field name="category_id" ref="uom_categ_subscription_user" />
			<field name="factor" eval="24" />
			<field name="uom_type">smaller</field>
		</record>
	</data>
</odoo>
