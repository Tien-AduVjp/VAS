<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="document_auto_generate_rule_hr_contract" model="document.auto.generate.rule" forcecreate="False">
        <field name="res_model_id" ref="hr_contract.model_hr_contract"/>
        <field name="workspace_field_id" ref="hr_contract.field_hr_contract__department_id"/>
        <field name="parent_workspace_id" ref="document_workspace_hr_contract"/>
        <field name="workspace_tag_ids" eval="[
            (4, ref('document_tag_hr_contract_contract')),
            (4, ref('document_tag_hr_contract_running')),
            (4, ref('document_tag_hr_contract_expired')),
            (4, ref('document_tag_hr_contract_cancel')),
            (4, ref('viin_document.document_tag_to_review')),
            (4, ref('viin_document.document_tag_approve')),
            (4, ref('viin_document.document_tag_deprecate'))]"/>
        <field name="workspace_read_team_ids" eval="[(4, ref('document_team_hr_contract'))]"/>
        <field name="workspace_write_group_ids" eval="[(4, ref('hr.group_hr_manager'))]"/>
        <field name="workspace_read_group_ids" eval="[(4, ref('hr.group_hr_user'))]"/>
        <field name="workspace_action_ids" eval="[
            (4, ref('document_action_hr_contract_running')),
            (4, ref('document_action_hr_contract_expired')),
            (4, ref('viin_document.document_action_ask_to_review')),
            (4, ref('viin_document.document_action_approve'))]"/>
    </record>

</odoo>
