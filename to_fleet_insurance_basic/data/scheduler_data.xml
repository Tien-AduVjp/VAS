<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="ir_cron_find_to_set_expire" model="ir.cron">
            <field name="name">Fleet Vehicle Insurance - Set Expiry</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">6</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field name="state">code</field>
            <field name="model_id" ref="model_fleet_vehicle_insurance" />
            <field name="code">model.action_cron_set_expired()</field>
        </record>
        
        <record id="ir_cron_send_expire_notice" model="ir.cron">
            <field name="name">Fleet Vehicle Insurance - Expiry Notification</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field name="state">code</field>
            <field name="model_id" ref="model_fleet_vehicle_insurance" />
            <field name="code">model.action_cron_send_expire_notice()</field>
        </record>
        
   </data>
</odoo>