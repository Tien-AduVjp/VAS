<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="view_zoom_user_report_form" model="ir.ui.view">
			<field name="name">zoom.user.report.form</field>
			<field name="model">zoom.user.report</field>
			<field name="priority" eval="8" />
			<field name="arch" type="xml">
				<form string="Zoom User Report">
					<sheet>
						<div>
							<group colspan="4" cols="4">
								<group>
									<field name="start_date" />
								</group>
								<group>
									<field name="end_date" />
								</group>
							</group>
						</div>
					</sheet>
					<footer>
						<button string="Print" type="object"
							name="gen_zoom_user_report" class="oe_highlight" />
						<button special="cancel" string="Close" type="object" />
					</footer>
				</form>
			</field>
		</record>
	</data>
</odoo>