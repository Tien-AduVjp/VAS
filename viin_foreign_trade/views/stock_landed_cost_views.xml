<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
    	<record id="view_stock_landed_cost_search" model="ir.ui.view">
			<field name="name">stock.landed.cost.search</field>
			<field name="model">stock.landed.cost</field>
			<field name="inherit_id" ref="stock_landed_costs.view_stock_landed_cost_search" />
			<field name="arch" type="xml">
				<field name="name" position="after">
					<field name="custom_declaration_import_id"/>
					<field name="custom_declaration_export_id"/>
				</field>
			</field>
		</record>
		<record id="act_export_custom_declaration_2_landed_cost"
			model="ir.actions.act_window">
			<field name="name">Landed Cost</field>
			<field name="res_model">stock.landed.cost</field>
			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="stock_landed_costs.view_stock_landed_cost_tree" />
			<field name="search_view_id"
				ref="stock_landed_costs.view_stock_landed_cost_search" />
			<field name="context">{'search_default_custom_declaration_export_id': active_id}</field>
		</record>
		<record id="act_import_custom_declaration_2_landed_cost"
			model="ir.actions.act_window">
			<field name="name">Landed Cost</field>
			<field name="res_model">stock.landed.cost</field>
			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="stock_landed_costs.view_stock_landed_cost_tree" />
			<field name="search_view_id"
				ref="stock_landed_costs.view_stock_landed_cost_search" />
			<field name="context">{'search_default_custom_declaration_import_id': active_id}</field>
		</record>
    </data>
</odoo>
