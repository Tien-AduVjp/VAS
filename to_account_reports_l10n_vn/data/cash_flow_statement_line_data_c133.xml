<?xml version="1.0" encoding="utf-8"?>
<odoo>
   	<!-- Cashflow Statement - Direct Method-->
	<record id="account_financial_report_line_cf_70_c133" model="account.financial.dynamic.report.line">
		<!-- Tiền và tương đương tiền cuối kỳ (70 = 50 + 60 + 61) -->
		<field name="name">Cash and cash equivalents, end of fiscal year (70 = 50 + 60 + 61)</field>
		<field name="code">CFS70_C133</field>
		<field name="report_code">70</field>
		<field name="formulas">balance = CFS50_C133.balance + CFS60_C133.balance + CFS61_C133.balance</field>
		<field name="sequence" eval="4" />
		<field name="financial_report_id" ref="af_dynamic_report_cashsummary_vn_c133" />
		<field name="level" eval="0" />
	</record>
	<record id="account_financial_report_line_cf_50_C133" model="account.financial.dynamic.report.line">
		<!-- Lưu chuyển tiền thuần trong kỳ (50 = 20 + 30 + 40) -->
		<field name="name">Net cash flows during the fiscal year (50 = 20+30+40)</field>
		<field name="code">CFS50_C133</field>
		<field name="report_code">50</field>
		<field name="formulas">balance = CFS20_C133.balance + CFS30_C133.balance + CFS40_C133.balance</field>
		<field name="sequence" eval="1" />
		<field name="parent_id" ref="account_financial_report_line_cf_70_c133" />
		<field name="level" eval="1" />
	</record>
	<record id="account_financial_report_line_cf_60_C133" model="account.financial.dynamic.report.line">
		<!-- Tiền và tương đương tiền đầu kỳ -->
		<field name="name">Cash and cash equivalents, beginning of fiscal year</field>
		<field name="code">CFS60_C133</field>
		<field name="report_code">60</field>
		<field name="formulas">balance = sum.balance</field>
		<field name="domain" eval="[('account_id.user_type_id', '=', ref('account.data_account_type_liquidity'))]" />
		<field name="groupby">account_id</field>
		<field name="sequence" eval="2" />
		<field name="parent_id" ref="account_financial_report_line_cf_70_c133" />
		<field name="level" eval="1" />
		<field name="special_date_changer">to_beginning_of_period</field>
	</record>
	<record id="account_financial_report_line_cf_61_C133" model="account.financial.dynamic.report.line">
		<!-- Ảnh hưởng của thay đổi tỷ giá hối đoái quy đổi ngoại tệ -->
		<field name="name">Effect of exchange rate fluctuations</field>
		<field name="code">CFS61_C133</field>
		<field name="report_code">61</field>
		<field name="formulas">balance = sum.balance</field>
		<field name="domain" eval="[('ctp_account_ids.code','=like','413%'),('account_id.user_type_id', '=', ref('account.data_account_type_liquidity'))]" />
		<field name="sequence" eval="3" />
		<field name="parent_id" ref="account_financial_report_line_cf_70_c133" />
		<field name="level" eval="1" />
		<field name="special_date_changer">strict_range</field>
	</record>
	<record id="account_financial_report_line_cf_CFS20_C133" model="account.financial.dynamic.report.line">
		<!-- Lưu chuyển tiền thuần từ hoạt động kinh doanh -->
		<field name="name">Net cash flows from operating activities</field>
		<field name="code">CFS20_C133</field>
		<field name="report_code">20</field>
		<field name="formulas">balance = CFS01_C133.balance + CFS06_C133.balance + CFS02_C133.balance + CFS03_C133.balance + CFS04_C133.balance + CFS05_C133.balance + CFS07_C133.balance</field>
		<field name="sequence" eval="1" />
		<field name="parent_id" ref="account_financial_report_line_cf_50_C133" />
		<field name="level" eval="2" />
		<field name="special_date_changer">strict_range</field>
	</record>
	<record id="account_financial_report_line_cf_CFSI_C133" model="account.financial.dynamic.report.line">
		<!-- Lưu chuyển tiền từ hoạt động kinh doanh -->
		<field name="name">I. Cash flows from operating activities</field>
		<field name="code">CFSI_C133</field>
		<field name="sequence" eval="1" />
		<field name="parent_id" ref="account_financial_report_line_cf_CFS20_C133" />
		<field name="level" eval="3" />
	</record>
	<record id="account_financial_report_line_cf_01_C133" model="account.financial.dynamic.report.line">
		<!-- Tiền thu từ bán hàng, cung cấp dịch vụ và doanh thu khác -->
		<field name="name">1. Proceeds from sales and services rendered and other revenues</field>
		<field name="code">CFS01_C133</field>
		<field name="report_code">01</field>
		<field name="formulas">balance = CFS01A_C133.balance + CFS01B_C133.balance</field>
		<field name="groupby">account_id</field>
		<field name="sequence" eval="2" />
		<field name="parent_id" ref="account_financial_report_line_cf_CFS20_C133" />
		<field name="level" eval="3" />
		<field name="special_date_changer">strict_range</field>
	</record>
	<record id="account_financial_report_line_cf_01a_C133" model="account.financial.dynamic.report.line">
		<!-- Tiền thu từ bán hàng, cung cấp dịch vụ và doanh thu khác (đối ứng trực tiếp 111/112) -->
		<field name="name">1a. Proceeds from sales and services rendered and other revenues (Cash Direct)</field>
		<field name="code">CFS01A_C133</field>
		<field name="report_code">01a</field>
		<field name="formulas">balance = -sum.balance</field>
		<field name="domain" eval="[('analytic_tag_ids','not in',[ref('l10n_vn_common.account_analytic_tag_fixed_assets'),ref('l10n_vn_common.account_analytic_tag_lending_loan'),ref('l10n_vn_common.account_analytic_tag_interests_dividends_distributed_profits')]),
									'|',('account_id.user_type_id','in',[ref('account.data_account_type_revenue'),ref('to_account_financial_income.account_account_type_financial_income')]),
										('ctp_aml_ids.ctp_account_ids.code','=like','121%'),
									'|',('ctp_account_ids.internal_type','=','liquidity'),
										('ctp_aml_ids.matched_credit_ids.credit_move_id.ctp_account_ids.internal_type','=','liquidity')]" />
		<field name="groupby">account_id</field>
		<field name="sequence" eval="3" />
		<field name="parent_id" ref="account_financial_report_line_cf_01_C133" />
		<field name="level" eval="4" />
		<field name="show_on_pdf" eval="False" />
		<field name="special_date_changer">strict_range</field>
	</record>
	<record id="account_financial_report_line_cf_01b_C133" model="account.financial.dynamic.report.line">
		<!-- Tiền thu từ bán hàng, cung cấp dịch vụ và doanh thu khác (đối ứng gián tiếp 113) -->
		<field name="name">1b. Proceeds from sales and services rendered and other revenues (via receivables)</field>
		<field name="code">CFS01B_C133</field>
		<field name="report_code">01b</field>
		<field name="formulas">balance = -sum.balance</field>
		<field name="domain" eval="[('account_id.internal_type','=','receivable'),
									('credit', '>', 0),
									('ctp_aml_ids.matched_credit_ids.credit_move_id.ctp_account_ids.internal_type','=','liquidity'),
									'|',('matched_debit_ids.debit_move_id.ctp_account_ids.code','=like','121%'),
										('matched_debit_ids.debit_move_id.ctp_account_ids.user_type_id','in',[ref('account.data_account_type_revenue'),ref('to_account_financial_income.account_account_type_financial_income')]),
    								('matched_debit_ids.debit_move_id.ctp_aml_ids.analytic_tag_ids','not in',[ref('l10n_vn_common.account_analytic_tag_fixed_assets'),ref('l10n_vn_common.account_analytic_tag_lending_loan'), ref('l10n_vn_common.account_analytic_tag_interests_dividends_distributed_profits')])
    					]" />
		<field name="groupby">account_id</field>
		<field name="sequence" eval="4" />
		<field name="parent_id" ref="account_financial_report_line_cf_01_C133" />
		<field name="level" eval="4" />
		<field name="special_date_changer">strict_range</field>
		<field name="show_on_pdf" eval="False" />
	</record>

	<record id="account_financial_report_line_cf_02_C133" model="account.financial.dynamic.report.line">
		<!-- Tiền chi trả cho người cung cấp hàng hóa, dịch vụ -->
		<field name="name">2. Expenditures paid to suppliers</field>
		<field name="code">CFS02_C133</field>
		<field name="report_code">02</field>
		<field name="formulas">balance = CFS02a_C133.balance + CFS02b_C133.balance</field>
		<field name="domain" eval="[]" />
		<field name="groupby">partner_id</field>
		<field name="sequence" eval="3" />
		<field name="parent_id" ref="account_financial_report_line_cf_CFS20_C133" />
		<field name="level" eval="3" />
		<field name="special_date_changer">strict_range</field>
	</record>
	<record id="account_financial_report_line_cf_02a_C133" model="account.financial.dynamic.report.line">
		<!-- Tiền chi trả cho người cung cấp hàng hóa, dịch vụ -->
		<field name="name">2.a. Expenditures paid to suppliers (Cash Direct)</field>
		<field name="code">CFS02a_C133</field>
		<field name="report_code">02a</field>
		<field name="formulas">balance = -sum.balance</field>
		<field name="domain" eval="[('debit','>',0),
									'|',
										'&amp;',
											'&amp;',('account_id.code','=like','15%'), ('ctp_aml_ids.full_reconcile_id','!=', False),
											'|', ('ctp_account_ids.code','=like','111%'), ('ctp_account_ids.code','=like','112%'),
										'&amp;',
											'&amp;','&amp;',('account_id.code','=like','1331%'), ('ctp_aml_ids.ctp_account_ids.code','=like','15%'), ('ctp_aml_ids.full_reconcile_id','!=', False),
											'|', ('ctp_account_ids.code','=like','111%'), ('ctp_account_ids.code','=like','112%')]" />
		<field name="groupby">account_id</field>
		<field name="sequence" eval="3" />
		<field name="parent_id" ref="account_financial_report_line_cf_02_C133" />
		<field name="level" eval="4" />
		<field name="show_on_pdf" eval="False" />
		<field name="special_date_changer">strict_range</field>
	</record>
	<record id="account_financial_report_line_cf_02b_C133" model="account.financial.dynamic.report.line">
		<!-- Tiền chi trả cho người cung cấp hàng hóa, dịch vụ -->
		<field name="name">2.b. Expenditures paid to suppliers (Via Payables)</field>
		<field name="code">CFS02b_C133</field>
		<field name="report_code">02b</field>
		<field name="formulas">balance = -sum.balance</field>
		<field name="domain" eval="[('account_id.code','=like','%331%'),('debit','>',0),('matched_credit_ids.credit_move_id.ctp_aml_ids.analytic_tag_ids','not in',[ref('l10n_vn_common.account_analytic_tag_fixed_assets')]),
    					'&amp;',
   							'|','|','|','|',
	   							('matched_credit_ids.credit_move_id.ctp_account_ids.code','=like','15%'),
	   							('matched_credit_ids.credit_move_id.ctp_account_ids.code','=like','3388%'),
	   							('matched_credit_ids.credit_move_id.ctp_account_ids.code','=like','133%'),
	   							('matched_credit_ids.credit_move_id.ctp_account_ids.code','=like','242%'),
	   							('matched_credit_ids.credit_move_id.ctp_account_ids.user_type_id','in',[ref('account.data_account_type_expenses')]),
   							'&amp;',
	   							('matched_credit_ids.credit_move_id.ctp_account_ids.code','not like','635%'),
	   							('matched_credit_ids.credit_move_id.ctp_account_ids.code','not like','821%')]" />
		<field name="groupby">account_id</field>
		<field name="sequence" eval="4" />
		<field name="parent_id" ref="account_financial_report_line_cf_02_C133" />
		<field name="level" eval="4" />
		<field name="show_on_pdf" eval="False" />
		<field name="special_date_changer">strict_range</field>
	</record>
	<record id="account_financial_report_line_cf_03_C133" model="account.financial.dynamic.report.line">
		<!-- Tiền chi trả cho người lao động -->
		<field name="name">3. Expenditures paid to employees</field>
		<field name="code">CFS03_C133</field>
		<field name="report_code">03</field>
		<field name="formulas">balance = -sum.balance</field>
		<field name="domain" eval="[('debit','>',0),('account_ids.code','=like','334%'),
									('ctp_aml_ids.matched_debit_ids.debit_move_id.ctp_account_ids.internal_type','=','liquidity')]" />
		<!-- <field name="counterpart_accounts">('334')</field>-->
		<field name="groupby">partner_id</field>
		<field name="sequence" eval="4" />
		<field name="parent_id" ref="account_financial_report_line_cf_CFS20_C133" />
		<field name="level" eval="3" />
		<field name="special_date_changer">strict_range</field>
	</record>
	<record id="account_financial_report_line_cf_04_C133" model="account.financial.dynamic.report.line">
		<!-- Tiền lãi vay đã trả -->
		<field name="name">4. Paid interests</field>
		<field name="code">CFS04_C133</field>
		<field name="report_code">04</field>
		<!-- Phân loại bằng thẻ quản trị Borrowing Loans để tránh nhầm lẫn sang hoạt động khác cũng hạch toán 635 (vd: lỗ tỷ giá)
    		 -->
		<field name="formulas">balance = -sum.balance</field>
		<field name="domain" eval="[
				'|',
					'&amp;','&amp;','&amp;',('debit','>',0),('account_id.code','=like','331%'),
	   					('matched_credit_ids.credit_move_id.ctp_aml_ids.analytic_tag_ids','in',[ref('l10n_vn_common.account_analytic_tag_borrowing_loan')]),
	   					'|','|',
	   						('matched_credit_ids.credit_move_id.ctp_account_ids.code','=like','635%'),
	   						('matched_credit_ids.credit_move_id.ctp_account_ids.code','=like','335%'),
	   						('ctp_account_ids.code','=like','242%'),
					'&amp;','&amp;',('analytic_tag_ids','in',[ref('l10n_vn_common.account_analytic_tag_borrowing_loan')]),
							('ctp_account_ids.internal_type','=','liquidity'),
							'|',('account_id.code','=like','635%'),
								('account_id.code','=like','335%')]" />
		<field name="groupby">account_id</field>
		<field name="sequence" eval="5" />
		<field name="parent_id" ref="account_financial_report_line_cf_CFS20_C133" />
		<field name="level" eval="3" />
		<field name="special_date_changer">strict_range</field>
	</record>
	<record id="account_financial_report_line_cf_05_C133" model="account.financial.dynamic.report.line">
		<!-- Thuế thu nhập doanh nghiệp đã nộp -->
		<field name="name">5. Paid enterprise income tax</field>
		<field name="code">CFS05_C133</field>
		<field name="report_code">05</field>
		<field name="formulas">balance = -sum.balance</field>
		<field name="domain" eval="[('account_id.code','=like','3334%'),('debit','>',0),
							'|','|',('ctp_account_ids.internal_type','=','liquidity'),
									('ctp_aml_ids.matched_debit_ids.debit_move_id.ctp_account_ids.internal_type','=','liquidity'),
									('ctp_aml_ids.matched_debit_ids.debit_move_id.ctp_aml_ids.matched_debit_ids.debit_move_id.ctp_account_ids.internal_type','=','liquidity')]" />
		<field name="groupby">account_id</field>
		<field name="sequence" eval="6" />
		<field name="parent_id" ref="account_financial_report_line_cf_CFS20_C133" />
		<field name="level" eval="3" />
		<field name="special_date_changer">strict_range</field>
	</record>
	<record id="account_financial_report_line_cf_06_C133" model="account.financial.dynamic.report.line">
		<!-- Tiền thu khác từ hoạt động kinh doanh -->
		<field name="name">6. Other proceeds from operating activities</field>
		<field name="code">CFS06_C133</field>
		<field name="report_code">06</field>
		<field name="formulas">balance = CFS06a_C133.balance - CFS06b_C133.balance</field>
		<field name="domain" eval="" />
		<field name="groupby">account_id</field>
		<field name="sequence" eval="7" />
		<field name="parent_id" ref="account_financial_report_line_cf_CFS20_C133" />
		<field name="level" eval="3" />
		<field name="special_date_changer">strict_range</field>
	</record>
	<record id="account_financial_report_line_cf_06a_C133" model="account.financial.dynamic.report.line">
		<!-- Tổng thu -->
		<field name="name">6.a. Total Cash Received</field>
		<field name="code">CFS06a_C133</field>
		<field name="report_code">06a</field>
		<field name="formulas">balance = sum.balance</field>
		<field name="domain" eval="[('debit','>',0),('account_id.internal_type','=','liquidity')]" />
		<field name="groupby">account_id</field>
		<field name="sequence" eval="1" />
		<field name="parent_id" ref="account_financial_report_line_cf_06_C133" />
		<field name="level" eval="4" />
		<field name="special_date_changer">strict_range</field>
		<field name="show_on_pdf" eval="False" />
	</record>
	<record id="account_financial_report_line_cf_06b_C133" model="account.financial.dynamic.report.line">
		<!-- Tổng thu -->
		<field name="name">6.b. Total Cash Received which was classified into other items</field>
		<field name="code">CFS06b_C133</field>
		<field name="report_code">06b</field>
		<field name="formulas">balance = CFS01_C133.balance + CFS22a_C133.balance + CFS22b_C133.balance + CFS24_C133.balance + CFS25_C133.balance + CFS31_C133.balance + CFS33_C133.balance</field>
		<field name="domain" eval="" />
		<field name="sequence" eval="2" />
		<field name="parent_id" ref="account_financial_report_line_cf_06_C133" />
		<field name="level" eval="4" />
		<field name="special_date_changer">strict_range</field>
		<field name="show_on_pdf" eval="False" />
	</record>
	<record id="account_financial_report_line_cf_07_C133" model="account.financial.dynamic.report.line">
		<!-- Tiền chi khác cho hoạt động kinh doanh -->
		<field name="name">7. Other expenditures on operating activities</field>
		<field name="code">CFS07_C133</field>
		<field name="report_code">07</field>
		<field name="formulas">balance = CFS07a_C133.balance - CFS07b_C133.balance</field>
		<field name="domain" eval="" />
		<field name="sequence" eval="8" />
		<field name="parent_id" ref="account_financial_report_line_cf_CFS20_C133" />
		<field name="level" eval="3" />
		<field name="special_date_changer">strict_range</field>
	</record>
	<record id="account_financial_report_line_cf_07a_C133" model="account.financial.dynamic.report.line">
		<!-- Tổng tiền chi -->
		<field name="name">7.a. Total Cash Out</field>
		<field name="code">CFS07a_C133</field>
		<field name="report_code">07a</field>
		<field name="formulas">balance = sum.balance</field>
		<field name="domain" eval="[('credit','>',0),('account_id.user_type_id', '=', ref('account.data_account_type_liquidity'))]" />
		<field name="groupby">account_id</field>
		<field name="sequence" eval="1" />
		<field name="parent_id" ref="account_financial_report_line_cf_07_C133" />
		<field name="level" eval="4" />
		<field name="special_date_changer">strict_range</field>
		<field name="show_on_pdf" eval="False" />
	</record>
	<record id="account_financial_report_line_cf_07b_C133" model="account.financial.dynamic.report.line">
		<!-- Tổng tiền chi từ các hoạt động đã phân loại -->
		<field name="name">7.b. Total Cash Out which was classified into other items</field>
		<field name="code">CFS07b_C133</field>
		<field name="report_code">07b</field>
		<field name="formulas">balance = CFS02_C133.balance + CFS03_C133.balance + CFS04_C133.balance + CFS05_C133.balance + CFS21_C133.balance + CFS22c_C133.balance + CFS22d_C133.balance + CFS23_C133.balance + CFS32_C133.balance + CFS34_C133.balance + CFS35_C133.balance</field>
		<field name="domain" eval="" />
		<field name="sequence" eval="2" />
		<field name="parent_id" ref="account_financial_report_line_cf_07_C133" />
		<field name="level" eval="4" />
		<field name="special_date_changer">strict_range</field>
		<field name="show_on_pdf" eval="False" />
	</record>
	<record id="account_financial_report_line_cf_30_C133" model="account.financial.dynamic.report.line">
		<!-- Lưu chuyển tiền thuần từ hoạt động đầu tư -->
		<field name="name">Net cash flows from investing activities</field>
		<field name="code">CFS30_C133</field>
		<field name="report_code">30</field>
		<field name="formulas">balance = CFS22_C133.balance + CFS21_C133.balance + CFS23_C133.balance + CFS24_C133.balance + CFS25_C133.balance</field>
		<field name="sequence" eval="2" />
		<field name="parent_id" ref="account_financial_report_line_cf_50_C133" />
		<field name="level" eval="2" />
		<field name="special_date_changer">strict_range</field>
	</record>
	<record id="account_financial_report_line_cf_CFSII_C133" model="account.financial.dynamic.report.line">
		<!-- Lưu chuyển tiền từ hoạt động đầu tư -->
		<field name="name">II. Cash flows from investing activities</field>
		<field name="code">CFSII_C133</field>
		<field name="sequence" eval="1" />
		<field name="parent_id" ref="account_financial_report_line_cf_30_C133" />
		<field name="level" eval="3" />
	</record>
	<record id="account_financial_report_line_cf_21_C133" model="account.financial.dynamic.report.line">
