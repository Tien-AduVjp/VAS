<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="einvoice_service_company_rule" model="ir.rule">
        <field name="name">Access einvoice services in own companies only</field>
        <field name="model_id" ref="l10n_vn_edi.model_einvoice_service"/>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>

    <record id="einvoice_serial_company_rule" model="ir.rule">
        <field name="name">Access einvoice serial in own companies only</field>
        <field name="model_id" ref="l10n_vn_edi.model_account_einvoice_serial"/>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>
</odoo>
