<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
	
		<record id="equity_range_tree_view" model="ir.ui.view" >
			<field name="name">res.partner.equity.range.tree</field>
			<field name="model">res.partner.equity.range</field>
			<field name="arch" type="xml">
				<tree string="Equity Range List">
					<field name="name" />
				</tree>
			</field>
		</record>
		
		<record id="equity_range_form_view" model="ir.ui.view" >
			<field name="name">res.partner.equity.range.form</field>
			<field name="model">res.partner.equity.range</field>
			<field name="arch" type="xml">
				<form string="Equity Range Form">
					<sheet>
						<div class="oe_title">
							<h1>
								<field name="name" />
							</h1>
						</div>
						<group>
							<group>
								<field name="description" />
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>
		
		<record id="equity_range_search_view" model="ir.ui.view" >
			<field name="name">res.partner.equity.range.search</field>
			<field name="model">res.partner.equity.range</field>
			<field name="arch" type="xml">
				<search string="Search Equity Range">
					<field name="name" />
				</search>
			</field>
		</record>
		
		<record id="equity_range_action" model="ir.actions.act_window" >
			<field name="name">Equity Range</field>
			<field name="res_model">res.partner.equity.range</field>
			<field name="view_mode">tree,form</field>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">Create a new Equity Range</p>
			</field>
		</record>
		
		<menuitem id="equity_range_menu" action="equity_range_action" parent="contacts.res_partner_menu_config" sequence="19"/>
	
	</data>
</odoo>