<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="ir_cron_ensure_download_einvoice_file" model="ir.cron">
			<field name="name">Ensure Download E-invoice Files</field>
			<field name="model_id" ref="model_account_move" />
			<field name="user_id" ref="base.user_root" />
			<field name="state">code</field>
			<field name="interval_number">5</field>
			<field name="interval_type">minutes</field>
			<field name="numbercall">-1</field>
			<field eval="False" name="doall" />
			<field name="code">model._cron_ensure_einvoice_download_file()
			</field>
		</record>
		<record id="ir_cron_auto_issue_einvoice" model="ir.cron">
			<field name="name">Auto Issue E-invoices</field>
			<field name="model_id" ref="model_account_move" />
			<field name="user_id" ref="base.user_root" />
			<field name="state">code</field>
			<field name="interval_number">15</field>
			<field name="interval_type">minutes</field>
			<field name="numbercall">-1</field>
			<field eval="False" name="doall" />
			<field name="code">model._cron_issue_einvoices()
			</field>
		</record>
	</data>
</odoo>