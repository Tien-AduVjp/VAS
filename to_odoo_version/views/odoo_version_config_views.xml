<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record model="ir.ui.view" id="odoo_version_config_form_view">
			<field name="name">odoo_version.config.form</field>
			<field name="model">odoo.version.config</field>
			<field name="arch" type="xml">
				<form string="Odoo Version Config Form">
					<header>
					</header>
					<sheet>
						<div class="oe_button_box" name="button_box"></div>
						<group>
							<group>
								<field name="section_id" />
								<field name="name" />
								<field name="value" />
							</group>
							<group>
								<field name="odoo_version_id" />
							</group>
						</group>
						<notebook>
						</notebook>
					</sheet>
					<div class="oe_chatter">
						<field name="message_follower_ids" widget="mail_followers" />
						<field name="message_ids" widget="mail_thread" />
					</div>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="odoo_version_config_tree_view">
			<field name="name">os.version.config.tree</field>
			<field name="model">odoo.version.config</field>
			<field name="arch" type="xml">
				<tree string="Odoo Version Configs List">
					<field name="name" />
					<field name="section_id" />
					<field name="odoo_version_id" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="odoo_version_config_search_view">
			<field name="name">odoo_version.config.search</field>
			<field name="model">odoo.version.config</field>
			<field name="arch" type="xml">
				<search string="Search Odoo Version Configs">
					<field name="name" />
					<field name="section_id" />
					<field name="odoo_version_id" />
				</search>
			</field>
		</record>


		<record model="ir.actions.act_window"
			id="odoo_version_config_action">
			<field name="name">Odoo Version Configs</field>
			<field name="res_model">odoo.version</field>
			<field name="view_mode">tree,form</field>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">Create a new Odoo Version Config</p>
			</field>
		</record>

	</data>
</odoo>

