<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="wizard_version_config_from_file_form_views"
			model="ir.ui.view">
			<field name="name">wizard.version.config.from.file.form</field>
			<field name="model">wizard.version.config.from.file</field>
			<field name="arch" type="xml">
				<form
					string="Generate Config. Directives for Odoo Version from File">
					<group>
						<group name="starting">
							<field name="odoo_version_id" invisible="1" />
							<field name="odoo_config_file_bin" filename="filename" />
							<field name="filename" invisible="1" />
							<field name="override_existing_value" />
						</group>
					</group>
					<footer>
						<button name="action_generate_config_from_file"
							string="Generate Config Directives" type="object"
							default_focus="1" class="oe_highlight" />
						or
						<button string="Cancel" class="oe_link" special="cancel" />
					</footer>
				</form>
			</field>
		</record>

		<record id="launch_version_config_from_file_wizard" model="ir.actions.act_window">
			<field name="name">Generate Config Directives from File</field>
			<field name="context">{'default_odoo_version_id': active_id}</field>
			<field name="view_mode">form</field>
			<field name="res_model">wizard.version.config.from.file</field>
			<field name="target">new</field>
		</record>
	</data>
</odoo>
