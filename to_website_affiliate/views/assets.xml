<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="0">
		<template id="to_website_affiliate_assets_frontend"
			inherit_id="website.assets_frontend" name="Affiliate JS">
			<xpath expr="." position="inside">
				<script type="text/javascript"
					src="/to_website_affiliate/static/src/js/website_affiliate.js"></script>
			</xpath>
		</template>

		<template id="to_website_affiliate_style"
			inherit_id="web.assets_frontend" name="Affiliate Style">
			<xpath expr="//link[last()]" position="after">
				<link href="/to_website_affiliate/static/src/scss/style.scss"
					type="text/scss" rel="stylesheet" />
			</xpath>
		</template>
	</data>
</odoo>
