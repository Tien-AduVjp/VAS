<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
	    <record id="sequence_picking_type_repair_supply" model="ir.sequence">
	        <field name="name">Picking Type Repair Supply</field>
	        <field name="code">picking.type.repair.supply</field>
	        <field name="prefix">PRS</field>
	        <field name="padding">5</field>
