<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="account_reports_backend"
			inherit_id="web.assets_backend" active="True">
			<xpath expr="." position="inside">
				<script type="text/javascript"
					src="/to_account_reports/static/src/js/af_filters.js"></script>
				<script type="text/javascript"
					src="/to_account_reports/static/src/js/af_reports.js"></script>
				<script type="text/javascript"
					src="/to_account_reports/static/src/js/af_report_followup.js"></script>
				<script type="text/javascript"
					src="/to_account_reports/static/src/js/af_report_dl.js"></script>
				<link rel="stylesheet" type="text/scss" href="/to_account_reports/static/src/scss/af_report.scss"/>
			</xpath>
		</template>
		<!-- stylesheets -->
		<template id="assets_financial_report">
			<t t-call="web._assets_helpers"><t t-set="no_bs_vars" t-value="True"/></t>

		    <t t-call="web._assets_bootstrap"/>
		    <link rel="stylesheet" type="text/scss" href="/web/static/src/scss/fonts.scss"/>
		    <link rel="stylesheet" type="text/scss" href="/to_account_reports/static/src/scss/af_report.scss"/>
		    <link rel="stylesheet" type="text/scss" href="/to_account_reports/static/src/scss/af_print.scss"/>
		</template>
    </data>
</odoo>
