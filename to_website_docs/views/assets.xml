<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<template id="assets_frontend" inherit_id="website.assets_frontend" name="Website Assets Frontend">
			<xpath expr="." position="inside">
				<link href="/to_website_docs/static/src/css/set1.css" rel="stylesheet" />
				<link href="/to_website_docs/static/src/css/set2.css" rel="stylesheet" />
				<link href="/to_website_docs/static/src/css/docs.snippet.css" rel="stylesheet" />
				<link href="/to_website_docs/static/src/css/docs.css" rel="stylesheet" />
				<script type="text/javascript" src="/to_website_docs/static/src/js/common.js" />
				<script type="text/javascript" src="/to_website_docs/static/src/js/docs.snippet.js"></script>
				<script type="text/javascript" src="/to_website_docs/static/src/js/docs.js"></script>
			</xpath>
		</template>

		<template id="assets_editor" name="Website Assets Editor" inherit_id="website.assets_editor">
			<xpath expr="." position="inside">
				<link href="/to_website_docs/static/src/css/evol-colorpicker.css" rel="stylesheet" />
				<script src="/to_website_docs/static/src/js/evol-colorpicker.js" />
				<script src="/to_website_docs/static/src/js/docs.snippet.editor.js" />
				<script src="/to_website_docs/static/src/js/docs.editor.js" />
			</xpath>
		</template>

		<template id="assets_app" inherit_id="website.layout">
			<xpath expr="//head/t[@t-css='false'][last()]" position="after">
				<script defer="defer" type="text/javascript" data-src="/to_website_docs/static/src/js/app.js"></script>
			</xpath>
		</template>
	</data>
</odoo>
