<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="document_auto_generate_rule_project_project_document" model="document.auto.generate.rule" forcecreate="False">
        <field name="res_model_id" ref="project.model_project_project"/>
        <field name="workspace_field_id" ref="project.field_project_project__name"/>
        <field name="parent_workspace_id" ref="document_workspace_project"/>
        <field name="workspace_tag_ids" eval="[
            (4, ref('viin_document.document_tag_to_review')),
            (4, ref('viin_document.document_tag_approve')),
            (4, ref('viin_document.document_tag_deprecate'))]"/>
        <field name="workspace_read_team_ids" eval="[(4, ref('document_team_project'))]"/>
        <field name="workspace_write_group_ids" eval="[(4, ref('project.group_project_manager'))]"/>
        <field name="workspace_read_group_ids" eval="[(4, ref('project.group_project_user'))]"/>
        <field name="workspace_action_ids" eval="[
            (4, ref('viin_document.document_action_ask_to_review')),
            (4, ref('viin_document.document_action_approve'))]"/>
    </record>

    <record id="document_auto_generate_rule_project_task_document" model="document.auto.generate.rule" forcecreate="False">
        <field name="res_model_id" ref="project.model_project_task"/>
        <field name="workspace_field_id" ref="project.field_project_task__project_id"/>
        <field name="parent_workspace_id" ref="document_workspace_project"/>
        <field name="workspace_tag_ids" eval="[
            (4, ref('viin_document.document_tag_to_review')),
            (4, ref('viin_document.document_tag_approve')),
            (4, ref('viin_document.document_tag_deprecate'))]"/>
        <field name="workspace_read_team_ids" eval="[(4, ref('document_team_project'))]"/>
        <field name="workspace_write_group_ids" eval="[(4, ref('project.group_project_manager'))]"/>
        <field name="workspace_read_group_ids" eval="[(4, ref('project.group_project_user'))]"/>
        <field name="workspace_action_ids" eval="[
            (4, ref('viin_document.document_action_ask_to_review')),
            (4, ref('viin_document.document_action_approve'))]"/>
    </record>

</odoo>
