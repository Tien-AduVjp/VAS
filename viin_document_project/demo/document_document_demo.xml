<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="document_document_srs1_demo" model="document.document">
        <field name="name">Office Design SRS</field>
        <field name="datas" type="base64" file="viin_document_project/demo/documents/srs1.doc"/>
        <field name="workspace_id" ref="document_workspace_project_office_design_demo"/>
        <field name="owner_id" ref="base.user_admin"/>
        <field name="tag_ids" eval="[
            (4, ref('document_tag_project_srs_demo')),
            (4, ref('viin_document.document_tag_to_review'))]"/>
    </record>

    <record id="document_document_srs2_demo" model="document.document">
