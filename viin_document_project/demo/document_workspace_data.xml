    <record id="document_workspace_project_office_design_demo" model="document.workspace">
        <field name="name">Office Design</field>
        <field name="parent_id" ref="document_workspace_project"/>
        <field name="tag_ids" eval="[
            (4, ref('document_tag_project_srs_demo')),
            (4, ref('document_tag_project_gap_demo')),
            (4, ref('viin_document.document_tag_to_review')),
            (4, ref('viin_document.document_tag_approve')),
            (4, ref('viin_document.document_tag_deprecate'))]"/>
        <field name="read_team_ids" eval="[(4, ref('document_team_project_office_design'))]"/>
        <field name="write_group_ids" eval="[(4, ref('project.group_project_manager'))]"/>
        <field name="write_group_ids" eval="[(4, ref('base.user_admin'))]"/>
        <field name="action_ids" eval="[
            (4, ref('viin_document.document_action_ask_to_review')),
            (4, ref('viin_document.document_action_approve'))]"/>
    </record>

