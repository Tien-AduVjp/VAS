<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="assets_financial_report">
		<t t-call="web._assets_helpers"><t t-set="no_bs_vars" t-value="True"/></t>
	    	<t t-call="web._assets_bootstrap"/>
	</template>
	<template id="print_template">
		<t t-raw="'&lt;base href=%s&gt;' % base_url"/>
		<html>
			<head>
			      <meta http-equiv="content-type" content="text/html; charset=utf-8" />
            		<t t-call-assets="to_stock_age_report.assets_financial_report" t-js="False"/>
			</head>
			<body class="o_af_reports_body_print">
			</body>
		</html>
	</template>
	

	<template id="stock_age_reports_backend"
		inherit_id="web.assets_backend" active="True">
		<xpath expr="." position="inside">
			<script type="text/javascript"
				src="/to_stock_age_report/static/src/js/stock_age_reports.js"></script>
			<script type="text/javascript"
				src="/to_stock_age_report/static/src/js/action_manager_stock_report_dl.js"></script>
		</xpath>
	</template>
</odoo>