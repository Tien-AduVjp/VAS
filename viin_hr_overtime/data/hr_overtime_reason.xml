<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
	<record id="hr_overtime_reason_general"
		model="hr.overtime.reason">
		<field name="name">General</field>
		<field name="description">For any overtime work that has no specific reason</field>
		<field name="company_id" eval="False" />
	</record>
	<record id="hr_overtime_reason_project"
		model="hr.overtime.reason">
		<field name="name">Project Deadline</field>
		<field name="description">For any overtime work dedicated for a project that has a very strict and closed deadline</field>
		<field name="company_id" eval="False" />
	</record>
</odoo>
