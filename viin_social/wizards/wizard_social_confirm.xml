<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="wizard_social_confirm_view_form" model="ir.ui.view">
        <field name="name">wizard.social.confirm</field>
        <field name="model">wizard.social.confirm</field>
        <field name="arch" type="xml">
            <form>
                <field name="message" />
                <footer>
                    <button string="Confirm" name="confirm" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>

